msgid "i0, i1"
msgstr "i0, i1"

#: src\inscriptions.md:115 src\inscriptions.md:118
msgid "1"
msgstr "1"

#: src\inscriptions.md:115
msgid "i2"
msgstr "i2"

#: src\inscriptions.md:116 src\inscriptions.md:117
msgid "3"
msgstr "3"

#: src\inscriptions.md:116
msgid "i3, i4, i5"
msgstr "i3, i4, i5"

#: src\inscriptions.md:118
msgid "4"
msgstr "4"

